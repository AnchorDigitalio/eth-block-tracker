  "main": "src/index.js",
  "scripts": {
    "test": "npm run test-node && npm run test-browser",
    "test-node": "browserify test/basic.js | node",
    "test-browser": "browserify test/basic.js | testling"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async-eventemitter": "^0.2.2",
    "babel-preset-env": "^1.3.2",
    "babelify": "^7.3.0",
    "eth-query": "^2.1.0",
    "ethjs-util": "^0.1.3",
    "json-rpc-engine": "^2.2.0",
    "pify": "^2.3.0",
    "tape": "^4.6.3"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env"
          ]
        }
      ]
    ]
